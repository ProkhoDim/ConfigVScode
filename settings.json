{
  "window.zoomLevel": 1,
  "css.validate": false,
  "scss.validate": false,
  "less.validate": false,
  "emmet.triggerExpansionOnTab": true,
  "terminal.integrated.shell.windows": "C:\\WINDOWS\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "auto-rename-tag.activationOnLanguage": ["javascriptreact"],
  "auto-close-tag.activationOnLanguage": ["javascript", "javascriptreact"],
  "workbench.sideBar.location": "left",
  "liveServer.settings.CustomBrowser": "chrome",
  "git.autofetch": true,
  "workbench.colorCustomizations": {
    "tab.activeBackground": "#000000", //"#282c34",
    "activityBar.background": "#000000",
    "sideBar.background": "#000",
    "editor.background": "#0000009b",
    "menu.background": "#000",
    "menu.selectionBackground": "#00ff006c",
    "terminal.background": "#000000"
  },
  "prettier.singleQuote": true,
  "prettier.trailingComma": "all",
  "prettier.proseWrap": "always",
  "editor.formatOnSave": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "emmet.extensionsPath": ["E:\\GoIT"],
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "launch": {},
  "files.autoSave": "onFocusChange",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "all",
  "editor.renderControlCharacters": false,
  "breadcrumbs.enabled": true,
  "workbench.iconTheme": "eq-material-theme-icons-palenight",
  "explorer.confirmDragAndDrop": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "workbench.colorTheme": "One Dark Pro"
}
